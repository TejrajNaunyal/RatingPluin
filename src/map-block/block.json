{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 3,
	"name": "create-block/map-block",
	"version": "0.1.0",
	"title": "Map Block",
	"category": "widgets",
	"icon": "location",
	"description": "A block to embed maps using a URL.",
	"supports": {
		"html": false,
		"align": ["wide", "full"]
	},
	"attributes": {
		"mapUrl": {
			"type": "string",
			"default": ""
		}
	},
	"textdomain": "map-block",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
